{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-clientapp",
            "command": "c:/nodejs/npm",
            "type": "process",
            "args": [
                "start",
                "--prefix",
                "${workspaceFolder}/FrontEnd/ClientApp"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-webapp",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/BackEnd/WebApp/WebApp.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-workflowapp",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/BackEnd/Workflow_App/Workflow_App.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}