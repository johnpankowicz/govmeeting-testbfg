{
  "Data": {
    "DefaultConnection": {
      "ConnectionString": "Server=(localdb)\\mssqllocaldb;Database=Govmeeting;Trusted_Connection=True;MultipleActiveResultSets=true"
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AppSettings": {
    "IsDevelopment": true,
    "DatafilesPath": "DATAFILES",
    "TestfilesPath": "TESTDATA",
    "DeleteProcessingFolderOnStartup": true,
    "FixasrSegmentSize": 60,
    "FixasrSegmentOverlap": 0,
    "RecordingSizeForDevelopment": 180,
    "ExitAfterOnceThroughWorkflow":  true,
    "UseAudioFileAlreadyInCloud": true,
    "GoogleCloudBucketName": "govmeeting-transcribe",
    "MaxWorkFileBackups": 20,
    "MoveIncomingFileAfterProcessing": false,
    "GoogleApplicationCredentials": "_SECRETS/TranscribeAudio.json",
    "RequireManagerApproval": false
  }
}
